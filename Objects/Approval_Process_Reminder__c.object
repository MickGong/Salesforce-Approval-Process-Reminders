    <validationRules>
        <fullName>Level_Cannot_be_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>Alert_Recipient_1_From_Level__c &lt; 0
|| Alert_Recipient_2_From_Level__c &lt; 0
|| Alert_Recipient_3_From_Level__c &lt; 0
|| Alert_Recipient_4_From_Level__c &lt; 0
|| Alert_Recipient_5_From_Level__c &lt; 0
|| Alert_Recipient_6_From_Level__c &lt; 0</errorConditionFormula>
        <errorMessage>The Alert Level cannot be negative number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Approval_Process</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Related_Approval_Process__c )
||  Related_Approval_Process__c  == &apos;None&apos;</errorConditionFormula>
        <errorMessage>Please select the Related Approval Process</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_Business_Hour</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK( Business_Hours__c )</errorConditionFormula>
        <errorMessage>Please setup the Business Hours</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Same_Recipients</fullName>
        <active>true</active>
        <errorConditionFormula>NOT ISBLANK( Additional_Recipient_1__c )
&amp;&amp; NOT ISBLANK( Additional_Recipient_2__c )
&amp;&amp; Additional_Recipient_1__c == Additional_Recipient_2__c</errorConditionFormula>
        <errorMessage>Recipient 1 cannot be the same as Recipient 2</errorMessage>
    </validationRules>
